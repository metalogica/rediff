{
  "name": "rediff",
  "displayName": "rediff",
  "description": "Elucidate Retoolâ„¢ Pull-Requests",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "filenames",
          "name": "Filenames"
        },
        {
          "id": "diffTypes",
          "name": "Diff Types"
        },
        {
          "id": "moduleTypes",
          "name": "Module Types"
        }
      ]
    },
    "commands": [
      {
        "command": "rediff.loadBranch",
        "title": "Rediff: Load Git Branch"
      }
    ],
    "configuration": {
      "title": "Retool Directory",
      "properties": {
        "rediff.retoolDir": {
          "type": "string",
          "default": "absolute/path/to/retool-repository/directory",
          "description": "The absolute path to your local Retool repository"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  }
}
